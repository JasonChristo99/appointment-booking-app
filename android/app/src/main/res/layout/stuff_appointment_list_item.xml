<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="132dp">

    <TextView
        android:id="@+id/aptDateStuff"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginLeft="28dp"
        android:layout_marginTop="16dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/aptHourStuff"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginLeft="28dp"
        android:layout_marginTop="12dp"
        android:text="TextView"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/aptDateStuff" />

    <TextView
        android:id="@+id/aptCustomerStuff"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginLeft="72dp"
        android:layout_marginTop="20dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/aptTypeStuff"
        app:layout_constraintTop_toBottomOf="@+id/aptDateStuff" />

    <TextView
        android:id="@+id/aptCleanerStuff"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginLeft="72dp"
        android:layout_marginTop="12dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/aptTypeStuff"
        app:layout_constraintTop_toBottomOf="@+id/aptHourStuff" />

    <ImageButton
        android:id="@+id/completeAptStuff"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="32dp"
        android:layout_marginRight="32dp"
        android:background="@android:color/transparent"
        android:scaleType="centerInside"
        android:tint="#979797"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/check_icon" />

    <TextView
        android:id="@+id/aptTypeStuff"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginLeft="28dp"
        android:layout_marginTop="12dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/aptHourStuff" />

    <ImageView
        android:id="@+id/imageViewStuff"
        android:layout_width="13dp"
        android:layout_height="15dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="4dp"
        android:layout_marginRight="4dp"
        android:tint="#979797"
        app:layout_constraintEnd_toStartOf="@+id/aptCustomerStuff"
        app:layout_constraintTop_toBottomOf="@+id/aptDateStuff"
        app:srcCompat="@drawable/contacts_icon" />

    <ImageView
        android:id="@+id/imageView2Stuff"
        android:layout_width="12dp"
        android:layout_height="15dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="4dp"
        android:layout_marginRight="4dp"
        android:tint="#979797"
        app:layout_constraintEnd_toStartOf="@+id/aptCleanerStuff"
        app:layout_constraintTop_toBottomOf="@+id/aptHourStuff"
        app:srcCompat="@drawable/paint_icon" />

</androidx.constraintlayout.widget.ConstraintLayout>